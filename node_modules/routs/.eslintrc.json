{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "no-bitwise": 0,
    "no-undef": 0,
    "no-continue": 0,
    "no-use-before-define": [
      "error",
      {
        "functions": false,
        "classes": true
      }
    ],
    "import/extensions": [2, "never", { "web.js": "never", "json": "never" }],
    "import/no-unresolved": [2, { "ignore": ["angular", "lodash"] }],
    "import/no-extraneous-dependencies": 0,
    "class-methods-use-this": 0,
    "new-cap" : 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": ["error", { "props": false }]
    }
}
